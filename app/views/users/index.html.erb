<h1>KarmaVille's Most Famous</h1>
<div class="pagination">
	<ul class="pagination">
		 <%= render 'pagination', :page => @page, :paginate =>@paginate %>
	</ul>
</div>
<table class="table table-bordered table-striped">
  <tr>
    <th>Karma</th>
    <th>Name</th>
    <th>Username</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
    <%= render 'user_row', :user => user %>
  <% end %>
</table>
